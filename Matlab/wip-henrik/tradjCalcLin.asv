function [qdot] = tradjCalcLin(x,y,z,tLim, J)
%tradjCalcLin Summary of this function goes here
%   Detailed explanation goes here

    t = sym('t');  
    
    vx = x*
    vy = (y(2)-y(1))/tLim;
    
    yFunc = y(1) + vy*t;
    %thetaxFunc = atan2(yFunc,z);
    
    wx = 1/(1+(yFunc/z)^2)*vy;
    xi = [0 vy 0 wx 0 0]';
    
    qdot = pinv(J)*xi;
    
end

