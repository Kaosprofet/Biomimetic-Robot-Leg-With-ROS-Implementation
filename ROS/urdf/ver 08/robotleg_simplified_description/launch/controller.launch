<launch>

<rosparam file="$(find robotleg_simplified_description)/launch/controller.yaml" command="load"/>
<node args="joint1_position_controller joint2_position_controller joint3_position_controller joint4_position_controller joint_state_controller " name="controller_spawner" ns="robotleg_simplified" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/robotleg_simplified/joint_states"/>
</node>

</launch>