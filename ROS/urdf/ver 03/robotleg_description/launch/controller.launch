<launch>

<rosparam file="$(find robotleg_description)/launch/controller.yaml" command="load"/>
<node args="motor1 &gt; joint1_position_controller motor2 &gt; joint2_position_controller motor3 &gt; joint3_position_controller motor4 &gt; joint4_position_controller joint_state_controller " name="controller_spawner" ns="robotleg" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/robotleg/joint_states"/>
</node>

</launch>
